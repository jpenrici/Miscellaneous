<externaltool id="Run_Python_external_terminal_">
    <description>Run simple files in Python.</description>
    <displayname>Run Python</displayname>
    <category></category>
    <executable output="showinpane" error="showinpane" modifiesdocument="yes">
        <path>python3</path>
        <arguments>-u %{CurrentDocument:FilePath}</arguments>
        <workingdirectory>%{CurrentDocument:Path}</workingdirectory>
    </executable>
</externaltool>
